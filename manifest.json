{
    "description": "Checks if the prime parts and the relics that contain them are vaulted. If so, they are tinted in gray.",
    "manifest_version": 2,
    "name": "Warframe Market Vaulted",
    "version": "0.1",

    "permissions": [
        "webRequest",
        "activeTab",
        "https://warframe.market/*",
        "https://switch.warframe.market/*",
        "https://ps4.warframe.market/*",
        "https://xbox.warframe.market/*",
        "https://api.warframe.market/v1/items/*"
    ],

    "background": {
        "scripts": ["background.js"]
    }
}